#: kivy 1.9.0

<ExportParametersPopup>
	input_grid: input_grid
	BoxLayout:
		size: root.size
		pos: root.pos
		orientation: 'vertical'
		GridLayout:
			size_hint: 1.0, 0.8
			cols: 1
			id: input_grid
		Button:
			size_hint: 1.0, 0.2
			text: 'Export'
			on_press: app.ExecuteExport(self, args[0])